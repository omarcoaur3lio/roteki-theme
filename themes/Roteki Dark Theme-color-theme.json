	"tokenColors": [
	  {
		"name": "Override and reset to defaults",
		"scope": [
		  "entity.name.variable",
		  "keyword.operator.assignment",
		  "meta.template",
		  "punctuation.accessor",
		  "support.constant",
		  "variable"
		],
		"settings": {
		  "fontStyle": "",
		  "foreground": "#c0c8ce"
		}
	  },
	  {
		"name": "De-emphasize comments",
		"scope": "comment",
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#3c4349"
		}
	  },
	  {
		"name": "Emphasize comment punctuation",
		"scope": "punctuation.definition.comment",
		"settings": {
		  "fontStyle": "",
		  "foreground": "#50575e"
		}
	  },
	  {
		"name": "Emphasize strings",
		"scope": "string",
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#9ddef8"
		}
	  },
	  {
		"scope": "punctuation.definition.string",
		"settings": {
		  "fontStyle": ""
		}
	  },
	  {
		"name": "Emphasize constants",
		"scope": ["constant.language", "constant.numeric"],
		"settings": {
		  "fontStyle": "",
		  "foreground": "#a2ca34"
		}
	  },
	  {
		"name": "De-emphasize most keywords",
		"scope": ["keyword - keyword.operator", "keyword.control", "storage"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#a2ca34"
		}
	  },
	  {
		"scope": "storage.type",
		"settings": {
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Emphasize operators",
		"scope": "keyword.operator",
		"settings": {
		  "foreground": "#c0c8ce"
		}
	  },
	  {
		"name": "Emphasize types",
		"scope": [
		  "entity.name.type",
		  "entity.name.class",
		  "support.type",
		  "support.class"
		],
		"settings": {
		  "fontStyle": "bold",
		  "foreground": "#c0c5ce"
		}
	  },
	  {
		"name": "De-emphasize `this` variables",
		"scope": "variable.language.this",
		"settings": {
		  "fontStyle": "bold",
		  "foreground": "#809fb6"
		}
	  },
	  {
		"name": "Emphasize template expression punctuation",
